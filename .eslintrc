{
  "globals": {
  },
  "env": {
    "node": true,
    "mocha": true,
    "amd": true,
    "browser": true,
    "jasmine": true,
    "jquery": true
  },
  "plugins": [
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "quotes": 0,
    "no-underscore-dangle": 0,
    "curly": [2, "multi-line"],
    "camelcase": [2, {"properties": "never"}],
    "space-in-parens": [2, "always"],
    "space-before-blocks": [2, "always"],
    "no-extra-parens": [2, "always"],
    "block-scoped-var": 2,
    "no-else-return": 2,
    "no-self-compare": 2,
    "radix": 2,
    "comma-style": [2, "last"],
    "indent": [2, 2]
  }
}
